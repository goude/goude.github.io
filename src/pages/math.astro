---
import BaseLayout from "../layouts/BaseLayout.astro";
import MathQuiz from "../components/MathQuiz.astro";

const title = "Math Practice";
const description = "Practice math with an interactive retro calculator";
const permalink = Astro.site?.href ?? "https://goude.se/math";
---

<BaseLayout
  title={title}
  description={description}
  permalink={permalink}
  current="math"
>
  <MathQuiz />
</BaseLayout>
