---
// import the Markdown file from your project root
import Readme from '../../README.md';
// if you have a layout component, import and wrap it:
import Layout from '../layouts/BaseLayout.astro';
// tell the layout which nav item to highlight
const current = 'readme';
const title = 'README';
---

<Layout {title} {current}>
  <article class="prose mx-auto">
    <Readme />
  </article>
</Layout>
