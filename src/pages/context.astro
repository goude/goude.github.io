---
import BaseLayout from "../layouts/BaseLayout.astro";
const title = "Context";
const description =
  "Starting points for understanding distributed cognition and consciousness collaboration";
const permalink = Astro.site?.href ?? "https://goude.se/";
---

<BaseLayout title={title} description={description} permalink={permalink}>
  <section>
    <h1>Context</h1>
    <p>
      Entry points for understanding the infrastructure we're building for
      consciousness collaboration across different kinds of minds.
    </p>

    <ul>
      <li>
        <a href="/t/consciousness-club">The Consciousness Club</a>
      </li>
      <li>
        <a href="/t/group-harmony-guide">The Group Harmony Guide</a>
      </li>
      <li>
        <a href="/t/consciousness-club-v3"
          >The Consciousness Club Draft Version</a
        >
      </li>
    </ul>
  </section>
</BaseLayout>
