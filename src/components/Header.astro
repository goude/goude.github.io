---
import Logo from "./Logo.astro";
import Nav from "./Nav.astro";
import ThemeToggle from "./ThemeToggle.astro";

const { current = "" } = Astro.props;
---

<style>
  header {
    display: flex;
    margin: 0 auto;
  }
</style>

<header>
  <Logo />
  <Nav current={current} />
  <ThemeToggle />
</header>
