{
  "name": "goude-github-io",
  "version": "1.11.1",
  "type": "module",
  "scripts": {
    "prepare-readme": "node scripts/prepare-readme.js",
    "dev": "npm run prepare-readme && astro dev",
    "start": "npm run prepare-readme && astro dev --host 0.0.0.0",
    "build": "npm run prepare-readme && astro check && astro build && node scripts/beautify-html.js",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "check": "npm run lint && npm run format:check && npm run test",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/mdx": "^4.3.8",
    "@astrojs/svelte": "^7.2.0",
    "astro": "^5.15.1",
    "reading-time": "^1.5.0",
    "rehype-external-links": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-smartypants": "^2.0.0",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/node": "^22.13.4",
    "js-beautify": "^1.15.4",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1"
  }
}
